{
  "FileRepository": {
    "RootDirectory": "file_repository",
    "IsAbsolutePath": false,
    "MaxFileSizeInBytes": 104857600, // 100 MB
    "AllowedFileExtensions": [".txt", ".json", ".md", ".mp3", ".mp4", ".jpg", ".jpeg", ".png", ".gif", ".csv"]
  },
  "OllamaApi": {
    "Host": ""
  },
  "McpApi": {
    "Host": "http://localhost:8080/api/mcp"
  },
  "ConnectionStrings": {
    "MySql": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
