{
  "FileRepository": {
    "RootDirectory": "file_repository",
    "IsAbsolutePath": false,
    "MaxFileSizeInBytes": 104857600,
    "AllowedFileExtensions": [".txt", ".json", ".md", ".mp3", ".mp4", ".jpg", ".jpeg", ".png", ".gif", ".csv"]
  },
  "ConnectionStrings": {
    "MySql": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
